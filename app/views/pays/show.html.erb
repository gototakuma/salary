<%= form_for(@pay, url: pays_show_path(params: {id: @user.id})) do |f| %>
  <%=  render 'shared/error_messages', object: @user %>
  
  <%= f.label :started %>
  <%= f.time_field :started, class: "form_control" %>

  <%= f.label :finished %>
  <%= f.time_field :finished, class: "form_control" %>
  
  <%= f.submit "保存", class: "btn btn-primary" %>
<%end%>
